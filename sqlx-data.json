      "parameters": {
        "Left": [
          "Text",
          "Int8",
          {
            "Custom": {
              "kind": {
                "Enum": [
                  "seed",
                  "peer",
                  "rpc",
                  "rest",
                  "grpc"
                ]
              },
              "name": "endpoint_kind"
            }
          }
        ]
      }
    },
    "query": "\n        WITH cte AS (\n        SELECT\n            chain_data ->> 'chain_id' as chain_id,\n            jsonb_array_elements(chain_data -> 'peers' -> $1) ->> 'id' as node_id,\n            jsonb_array_elements(chain_data -> 'peers' -> $1) ->> 'address' as address,\n            jsonb_array_elements(chain_data -> 'peers' -> $1) ->> 'provider' as provider\n            FROM chain\n            WHERE id = $2\n        )\n        INSERT INTO endpoint (chain_id, address, provider, kind)\n        SELECT\n            chain_id,\n            CONCAT(node_id, '@', address),\n            COALESCE(provider, 'Unknown'),\n            $3\n        FROM cte\n        WHERE chain_id IS NOT NULL AND node_id IS NOT NULL AND address IS NOT NULL\n        ON CONFLICT (address, kind) DO UPDATE SET provider = EXCLUDED.provider\n        RETURNING id\n    "
  },
